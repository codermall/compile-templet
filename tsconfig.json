{
  "compilerOptions": {
    "module": "es2020",
    "target": "ES5",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "noEmit": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}